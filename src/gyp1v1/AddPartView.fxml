               <padding>
                  <Insets bottom="20.0" top="10.0" />
               </padding>
            </Label>
            <Label fx:id="companyNameLabel" layoutX="-8.0" layoutY="289.0" prefHeight="25.0" prefWidth="100.0" text="Company Name">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="20.0" top="10.0" />
               </padding>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="225.0" layoutY="436.0" prefHeight="50.0" prefWidth="178.0">
         <children>
            <Button layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#onActionSavePart" prefHeight="25.0" prefWidth="63.0" text="Save" />
            <Button layoutX="104.0" layoutY="13.0" mnemonicParsing="false" onAction="#CancelButtonPushed" prefHeight="25.0" prefWidth="63.0" text="Cancel" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="158.0" layoutY="87.0" prefHeight="349.0" prefWidth="234.0">
