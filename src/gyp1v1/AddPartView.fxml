               <HBox.margin>
                  <Insets left="40.0" right="40.0" top="10.0" />
               </HBox.margin>
               <toggleGroup>
                  <ToggleGroup fx:id="partToggleGroup" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="outsourceRadioButton" mnemonicParsing="false" onAction="#radioSelection" selected="true" text="Outsourced" toggleGroup="$partToggleGroup">
